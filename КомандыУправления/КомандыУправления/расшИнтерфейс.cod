extends МашинаРеквизитов.КомандыУправления.Интерфейс;

inclass public

  proc СоздатьКоманды;
    extended СоздатьКоманды;
    СоздатьКоманды_ЗПиДД_ОтчетностьПР366;
  end;

  proc СоздатьПанели;
    extended СоздатьПанели;
    РасширитьПанель_ЗПиДД_ПР366;
  end;

inclass private

  proc СоздатьКоманды_ЗПиДД_ОтчетностьПР366;
    var IconsPathSelfPrj: string;
    IconsPathSelfPrj = BaseInfo.ProjectByName['Бюджет_ЗПиДД'].Folder + 'Приложения\Иконки\';
    with МашинаРеквизитов.КомандыУправления.ОткрытьКартотеку do
      Добавить('ЗПиДД.Отчетность.СИоЗП', 'СИоЗП', Бюджет_ЗПиДД_ПР366.Отчетность.картСИоЗП, nil, IconsPathSelfPrj + '32\report_book');
    end;
  end;

  proc РасширитьПанель_ЗПиДД_ПР366;
    --var refCommand, newCommand: Command;
    with МашинаРеквизитов.КомандыУправления.ПанельКоманд.Взять('Отчеты') do
      with ВзятьГруппу('ОтчетыТиповые') do
        ДобавитьКоманды(['ЗПиДД.Отчетность.СИоЗП']);
      end;
    end;
  end;

end